<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ecjtu.zwh.mapper.AlbumMapper">
    <!-- List<Album> getAllAlbumByUserId(Integer userId) -->
    <select id="getAllAlbumByUserId" resultType="com.ecjtu.zwh.entity.Album">
        select * from album where user_id=#{userId}
    </select>
</mapper>